{
  "manifest_version": 3,
  "name": "YouTube Discord Presence",
  "version": "1.0",
  "permissions": [
    "tabs",
    "scripting",
    "nativeMessaging"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
